	<parameter name="CODIGO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT api.municipio.nome AS municipio,
	api.provincia.nome AS provincia,
	api.turma.nome AS turma,
	api.turma.classe,
	api.turma.periodo,
	api.cursos.nome AS curso,
	api.matricula.ano_academico,
	api.matricula.bairro,
	api.matricula.bi,
	api.matricula.data_nascimento,
	api.matricula.email,
	api.matricula.genero,
	api.matricula.nome AS nomeAluno,
	api.matricula.nome_mae,
	api.matricula.nome_pai,
	api.matricula.numero_estudante,
	api.matricula.numcrm,
	api.matricula.situacao,
	api.matricula.sobrenome,
	api.matricula.telefone,
	api.matricula.codigo as codigo
FROM api.matricula
	INNER JOIN api.turma ON 
	 api.matricula.turma_codigo = api.turma.codigo 
	INNER JOIN api.municipio ON 
	 api.matricula.municipio_codigo = api.municipio.codigo 
	INNER JOIN api.provincia ON 
	 api.matricula.provincia_codigo = api.provincia.codigo 
	INNER JOIN api.cursos ON 
	 api.turma.curso_codigo = api.cursos.codigo 
WHERE 
	 api.matricula.codigo = $P{CODIGO}]]>
	</queryString>
