	<parameter name="CODIGOTURMA" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT api.matricula.nome AS aluno,
	api.matricula.sobrenome,
	api.matricula.genero,
	api.matricula.numero_estudante,
	api.turma.nome AS turma,
	api.turma.classe,
	api.cursos.nome AS curso,
	api.turma.periodo,
	api.matricula.ano_academico,
	matricula.codigo as totalAlunos
FROM api.matricula
	INNER JOIN api.turma ON 
	 api.matricula.turma_codigo = api.turma.codigo 
	INNER JOIN api.cursos ON 
	 api.turma.curso_codigo = api.cursos.codigo
	 	 WHERE 
	 api.matricula.turma_codigo = $P{CODIGOTURMA}
]]>
